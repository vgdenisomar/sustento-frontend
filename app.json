{
  "expo": {
    "name": "Sustento",
    "slug": "Sustento",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoSustento.png",
    "splash": {
      "image": "./assets/splashSustento.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.sustento.sustento",
      "enableDangerousExperimentalLeanBuilds": true
    }
  }
}
